class Computer {
  Computer() {
    System.out.println("Constructor of Computer class.");
  }
 
  void computer_method() {
    System.out.println("Power gone! Shut down your PC soon...");
  }
 
  public static void main(String[] args) {
    Computer my = new Computer();
   Tablet your = new Tablet();
   
    my.computer_method();
    your.Tablet_method();
  }
}
class Tablet {
  Computer() {
    System.out.println("Constructor of Tablet class.");
  }
  void Tablet_method() {
    System.out.println("Power gone! Shut down your Tablet soon...");
  }
 
